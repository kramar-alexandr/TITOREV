globalprocedure NewPORn(record RcVc RepSpec)begin	record POVc POr;	row POVc POrw;	integer rwcnt, i;	string 20 frst,tost;	Boolean TrHs, TrHs2;	startreportnoheaderjob(""); 	startformat(15);	OutString(5,0,"Item",false);	OutString(60,0,"Qty",false);	OutString(100,0,"Rcvd.",false);	OutString(150,0,"Rcvd. OK",false);	OutString(200,0,"Qty - Rcvd.",false);	OutString(250,0,"Qty - Rcvd. OK",false);	endformat;		TrHs = true;		while(LoopMain(POr,1,TrHs)) begin			rwcnt = MatRowCnt(POr);				for(i=0;i<rwcnt;i=i+1) begin					TrHs2=true;			MatRowGet(POr,i,POrw);						if ((nonblank(RepSpec.f1))and(POrw.ArtCode!=RepSpec.f1)) then begin				TrHs2 = false;			end;						if (TrHs2==true) then begin						startformat(15);			OutString(5,0,POrw.ArtCode,false);			OutString(60,0,POrw.Quant,false);			OutString(100,0,POrw.Shipd1,false);			OutString(150,0,POrw.Shipd2,false);			OutString(200,0,POrw.Quant - POrw.Shipd1,false);			OutString(250,0,POrw.Quant - POrw.Shipd2,false);			endformat;						end;		end;	end;		endjob;end;